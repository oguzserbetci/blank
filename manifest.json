{
    "manifest_version": 2,
    "name": "blank",
    "version": "0.0.1",
    "description": "Creates exercises for your reading online!",
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["*://*/*"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "browser_action": {
        "default_icon":  "wink.png",
        "default_title": "blank__"
    }
}
