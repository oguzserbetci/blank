{
    "manifest_version": 2,
    "name": "blank",
    "version": "0.0.2",
    "description": "Creates exercises for your reading online!",
    "options_ui": {
        "page": "options.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "browser_action": {
        "default_icon":  "wink.png"
    }
}
